<%= stylesheet_link_tag 'calendar', media: 'all', 'data-turbolinks-track': 'reload' %>
<turbo-frame id="calendar-container">
  <div class="container">
    <div id="calendar"></div>
  </div>
</turbo-frame>

<%= render 'form', event: @event %>
<%= render 'show', event: @event %>
<%= render 'add', event: @event %>
<%= render 'setting', event: @event %>

<%= javascript_include_tag 'fullcalendar', "data-turbo-track": "reload", type: "module" %>
